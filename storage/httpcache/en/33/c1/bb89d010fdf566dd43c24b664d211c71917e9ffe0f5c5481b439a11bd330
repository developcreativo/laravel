            </script>        <script src="http://localhost/assets/js/plugins/dropzonejs/dropzone.min.js"></script>

    <script>
        Dropzone.options.excel = {
            init: function () {
                this.on("success", function (file) {
                    $.notify({
                        title: "<strong>Respuesta:</strong> ",
                        message: 'documento recibido correctamente',
                        icon: 'fa fa-check'}, {
                        type: 'success'
                    });
                });
                this.on("error", function (file) {
                    $.notify({
                        title: "<strong>Respuesta:</strong> ",
                        message: 'Lo siento el archivo no es un excel',
